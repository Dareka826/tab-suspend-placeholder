{
	"manifest_version": 2,
	"name": "Tab Suspend Placeholder",
	"version": "1.0",
	"description": "Adds an option to replace a tab with a light placeholder tab",

	"icons": {
		"32": "icon-32.png"
	},

	"permissions": [
		"tabs"
	],

	"browser_action": {
		"default_icon": "icon-32.png",
		"default_title": "Tab Suspend Placeholder",
		"default_popup": "popup/popup.html"
	}
}
